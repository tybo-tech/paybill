@model IEnumerable<WaterBillAppCore.Models.Setting>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1>Water charge per</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RangeStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RangeFinish)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChargeRate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RangeStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RangeFinish)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChargeRate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SettingId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SettingId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SettingId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
